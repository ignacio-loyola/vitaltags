{
  "name": "vitaltags-v2",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "pnpm --filter @vitaltags/web dev",
    "build": "pnpm --filter @vitaltags/web build",
    "start": "pnpm --filter @vitaltags/web start",
    "lint": "pnpm --filter @vitaltags/web lint",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "test": "vitest run",
    "test:ui": "vitest",
    "e2e": "playwright test"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "@playwright/test": "^1.46.0",
    "eslint": "^9.9.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "prisma": "^6.14.0"
  }
}
