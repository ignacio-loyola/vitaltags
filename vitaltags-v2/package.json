{
  "name": "vitaltags-v2",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "dev": "pnpm --filter @vitaltags/web dev",
    "build": "pnpm --filter @vitaltags/web build",
    "start": "pnpm --filter @vitaltags/web start",
    "lint": "pnpm --filter @vitaltags/web lint",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "seed": "tsx scripts/seed.ts",
    "test": "vitest run",
    "test:ui": "vitest",
    "e2e": "playwright test"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "@playwright/test": "^1.46.0",
    "eslint": "^9.9.0",
    "prettier": "^3.3.3",
    "tsx": "^4.20.4",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "@lucia-auth/adapter-prisma": "^4.0.1",
    "@prisma/client": "^6.14.0",
    "@simplewebauthn/browser": "^13.1.2",
    "@simplewebauthn/server": "^13.1.2",
    "@t3-oss/env-nextjs": "^0.13.8",
    "libsodium-wrappers": "^0.7.15",
    "lucia": "^3.2.2",
    "paseto-ts": "^2.0.5",
    "prisma": "^6.14.0",
    "zod": "^4.0.17"
  }
}
